#include <iostream>
using namespace std;

int main() {
    int t1, t2, t3; // Declare temperature variables

    // Get the first temperature reading
    cout << "Enter first temperature: ";
    cin >> t1;

    cout << "Enter second temperature: ";
    cin >> t2;

    cout << "Enter third temperature: ";
    cin >> t3;

    return 0; // End program
}
